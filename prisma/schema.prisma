generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model products {
  id                 Int  @id @default(autoincrement())
  products_name_id   Int?
  shelfs_sections_id Int?
  quantity_item      Int?

  @@index([products_name_id], map: "products_products_name_id_fkey")
  @@index([shelfs_sections_id], map: "products_shelfs_sections_id_fkey")
}

model products_name {
  id      Int     @id @default(autoincrement())
  name    String? @db.VarChar(64)
  barcode Int?
}

model shelfs_sections {
  id      Int     @id @default(autoincrement())
  shelf   String? @db.VarChar(1)
  section Int?
}
